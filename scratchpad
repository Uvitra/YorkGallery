let currentBackgroundGalleryImages = 0

function changeTheBackgroundImage() {
  backgroundGalleryImages[currentBackgroundGalleryImages]
  if (currentBackgroundGalleryImages >= backgroundGalleryImages.length) {
    currentBackgroundGalleryImages = 0
  }
  currentBackgroundGalleryImages++
}

setInterval(changeTheBackgroundImage, 1000)
